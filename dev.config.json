{
  "name": "E-tsena Development Configuration",
  "version": "2.0.0",
  "description": "Configuration d'exécution pour l'environnement de développement E-tsena",
  "development": {
    "database": {
      "type": "SQLite",
      "location": "local",
      "auto_migrate": true
    },
    "ui": {
      "theme": "modern",
      "colorScheme": "indigo_violet",
      "animations": true
    },
    "features": {
      "search": true,
      "filters": true,
      "statistics": true,
      "reports": true
    }
  }
}
